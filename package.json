{
  "name": "nmrium",
  "description": "React component to display and process nuclear magnetic resonance (NMR) spectra.",
  "version": "0.10.1",
  "license": "MIT",
  "main": "lib-cjs/component/NMRium.js",
  "module": "lib/component/NMRium.js",
  "types": "lib/component/NMRium.d.ts",
  "files": [
    "lib",
    "lib-cjs"
  ],
  "scripts": {
    "build": "react-scripts --max_old_space_size=4096 build",
    "build-with-profiling": "npm run build -- --profile",
    "check-types": "tsc --noEmit",
    "compile": "tsc --project tsconfig.esm.json && tsc --project tsconfig.cjs.json",
    "clean-lib": "rimraf lib lib-cjs",
    "dev": "react-scripts start",
    "docs": "markdown-folder-to-html docs",
    "eslint": "eslint . --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "prepublishOnly": "npm run clean-lib && npm run compile",
    "start": "react-scripts start ",
    "test": "npm run test-only && npm run eslint && npm run check-types",
    "test-only": "react-scripts test --watchAll=false"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "bugs": {
    "url": "https://github.com/cheminfo/nmrium/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/nmrium.git"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.1.5",
    "cheminfo-font": "^1.3.0",
    "d3": "^6.6.2",
    "eventemitter3": "^4.0.7",
    "file-saver": "^2.0.5",
    "formik": "^2.2.6",
    "immer": "^9.0.1",
    "lodash": "^4.17.21",
    "mf-parser": "^1.1.10",
    "ml-airpls": "^1.0.0",
    "ml-array-max": "^1.2.3",
    "ml-array-median": "^1.1.5",
    "ml-array-xy-equally-spaced": "^1.2.0",
    "ml-baseline-correction-regression": "^1.0.0",
    "ml-conrec": "^3.2.0",
    "ml-fft": "^1.3.5",
    "ml-gsd": "^6.6.1",
    "ml-regression-polynomial": "^2.1.0",
    "ml-spectra-processing": "^5.9.0",
    "multiplet-analysis": "^1.0.0",
    "nmr-correlation": "^1.2.0",
    "nmr-parser": "^1.0.1",
    "nmr-processing": "^1.2.0",
    "numeral": "^2.0.6",
    "openchemlib": "^7.4.0",
    "prismjs": "^1.23.0",
    "prop-types": "^15.7.2",
    "re-resizable": "^6.9.0",
    "react-card-flip": "^1.1.0",
    "react-color": "2.17.2",
    "react-draggable": "^4.4.3",
    "react-dropzone": "^11.3.2",
    "react-icons": "^4.2.0",
    "react-inspector": "^5.1.0",
    "react-jsgraph": "^2.0.1",
    "react-mf": "^2.0.1",
    "react-ocl": "^4.3.3",
    "react-ocl-nmr": "^1.0.0",
    "react-rnd": "^10.2.4",
    "react-simple-code-editor": "^0.11.0",
    "react-slider": "^1.1.4",
    "react-split-pane": "^0.1.92",
    "react-table": "7.6.0",
    "react-transition-group": "^4.4.1",
    "react-use": "^17.2.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/d3": "^6.3.0",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/prismjs": "^1.16.4",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@zakodium/eslint-config": "^3.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "markdown-folder-to-html": "^2.4.0",
    "modern-normalize": "^1.0.0",
    "prettier": "^2.2.1",
    "rc-menu": "^7.5.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "reactstrap": "^8.9.0",
    "rimraf": "^3.0.2",
    "typescript": "~4.2.3"
  },
  "volta": {
    "node": "14.16.0",
    "npm": "6.14.12"
  }
}
