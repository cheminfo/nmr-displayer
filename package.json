  "scripts": {
    "build": "react-scripts --max_old_space_size=4096 build",
    "build-with-profiling": "react-scripts --max_old_space_size=4096 build --profile",
    "check-types": "tsc --noEmit",
    "compile": "tsc --project tsconfig.esm.json && tsc --project tsconfig.cjs.json",
    "clean-lib": "rimraf lib lib-cjs",
    "dev": "react-scripts start",
    "docs": "markdown-folder-to-html docs",
    "eslint": "eslint src --ext js,jsx,ts,tsx --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "prepublishOnly": "npm run clean-lib && npm run compile",
    "start": "react-scripts start ",
    "test": "react-scripts test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
